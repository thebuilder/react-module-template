{
  "name": "react-module-template",
  "version": "0.1.3",
  "description": "Template for creating a new React module, ready to publish to np",
  "main": "lib/index.js",
  "author": "Daniel Schmidt",
  "repository": {
    "type": "git",
    "url": "https://github.com/thebuilder/react-module-template"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "component",
    "template",
    "boilerplate",
    "start",
    "prettier",
    "jest"
  ],
  "scripts": {
    "build:lib": "rm -rf lib && NODE_ENV=development babel src --out-dir lib --ignore __*,*.story.js,*.test.js",
    "build:storybook": "build-storybook --output-dir example",
    "deploy": "npm run build:storybook && git-directory-deploy --directory example --branch gh-pages",
    "dev": "concurrently -k -r 'jest --watch' 'npm run storybook'",
    "lint": "eslint src/.",
    "postinstall": "node setup/init.js",
    "precommit": "lint-staged",
    "prepublish": "npm run build:lib && node scripts/docs.js 'src/index.js'",
    "pretty": "prettier 'src/**/*.js' --write --no-semi --single-quote --trailing-comma all",
    "storybook": "start-storybook -p 9000",
    "test": "eslint src/. && jest"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write --no-semi --single-quote --trailing-comma all",
      "eslint",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier",
      "prettier/react"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "dependencies": {},
  "peerDependencies": {
    "prop-types": "^15.5.8",
    "react": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.2",
    "babel-preset-react-app": "^3.0.0",
    "concurrently": "^3.4.0",
    "del": "^2.2.2",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.0.0",
    "eslint-config-react-app": "^1.0.2",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.0",
    "eslint-plugin-react": "^7.0.0",
    "execa": "^0.6.3",
    "git-directory-deploy": "^1.5.1",
    "husky": "^0.13.3",
    "jest": "^20.0.0",
    "jsonfile": "^3.0.0",
    "lint-staged": "^3.4.1",
    "lodash": "^4.17.4",
    "minimist": "1.2.0",
    "npm-run-all": "4.0.2",
    "pascal-case": "^2.0.1",
    "plop": "^1.7.4",
    "prettier": "^1.3.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-docgen": "^2.15.0",
    "react-docs-markdown": "^0.1.7",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4"
  }
}
